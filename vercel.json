{
    "version": 2,
    "builds": [
      {
        "src": "run.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "run.py"
      }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DATABASE_URL": "@database_url",
        "SUPABASE_URL": "@supabase_url",
        "SUPABASE_KEY": "@supabase_key",
        "SECRET_KEY": "@secret_key",
        "ENCRYPTION_KEY": "@encryption_key"
    }
  }